{
  "name": "ai-invoice",
  "version": "0.7.0",
  "private": false,
  "author": "Lukas Nydegger",
  "description": "ai-invoicing",
  "license": "MIT",
  "main": "electron.js",
  "homepage": "./",
  "repository": "Famoose/ai-invoice",
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@fontsource/roboto": "^4.5.0",
    "@fontsource/roboto-slab": "^4.5.1",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@react-pdf/renderer": "^2.0.19",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-paginate": "^7.1.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "update-electron-app": "^2.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \" set BROWSER=none && npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "publish": "craco build && electron-forge publish",
    "package": "craco build && electron-forge package",
    "make": "craco build && electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "autoprefixer": "^9.8.6",
    "concurrently": "^6.2.1",
    "electron": "^14.0.1",
    "electron-devtools-installer": "^3.2.0",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "wait-on": "^5.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ai_invoice"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Famoose",
              "name": "ai-invoice"
            },
            "prerelease": true
          }
        }
      ]
    }
  }
}
